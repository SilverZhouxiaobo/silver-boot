<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.silver.framework.system.mapper.SysSerialNumberMapper">
    <resultMap type="cn.silver.framework.system.domain.SysSerialNumber" id="BaseResult">
        <result property="id" column="id"/>
        <result property="name" column="name"/>
        <result property="businessName" column="business_name"/>
        <result property="businessCode" column="business_code"/>
        <result property="prefix" column="prefix"/>
        <result property="dateFormatStr" column="date_format_str"/>
        <result property="numberExpression" column="number_expression"/>
        <result property="configTemplet" column="config_templet"/>
        <result property="maxSerial" column="max_serial"/>
        <result property="preMaxNum" column="pre_max_num"/>
        <result property="lastGetTime" column="last_get_time"/>
        <result property="autoIncrement" column="auto_increment"/>
        <result property="enable" column="enable"/>
    </resultMap>
    <select id="selectByBusinessCode" resultMap="BaseResult">
        select *
        from SYS_SERIAL_NUMBER
        where BUSINESS_CODE = #{businessCode}
    </select>
</mapper>